{
	"name": "fastify-route-group",
	"version": "1.0.2",
	"description": "Grouping and inheritance of paths",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"watch": "rollup -c -w",
		"build": "rimraf dist/* && cross-env NODE_ENV=production rollup -c",
		"test": "jest --collect-coverage",
		"lint": "eslint --fix ./src",
		"prettier": "prettier --ignore-unknown --write .",
		"beautifier": "npm run lint && npm run prettier",
		"prepublishOnly": "npm run test && npm run build",
		"example:typescript": "npx ts-node example/server.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TakNePoidet/fastify-route-group.git"
	},
	"keywords": [
		"fastify",
		"require",
		"folder",
		"plugin",
		"plugins",
		"routes",
		"group"
	],
	"author": "TakNePoidet <TakNePoidet@yandex.ru>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TakNePoidet/fastify-route-group/issues"
	},
	"homepage": "https://github.com/TakNePoidet/fastify-route-group#readme",
	"files": [
		"dist/"
	],
	"devDependencies": {
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-typescript": "^7.15.0",
		"@rollup/plugin-typescript": "^8.2.5",
		"@taknepoidet-config/eslint-config": "^1.6.0",
		"@taknepoidet-config/prettier": "^1.6.0",
		"@types/jest": "^26.0.24",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin-jsdoc": "^36.0.6",
		"jest": "^27.0.6",
		"nodemon": "^2.0.12",
		"prettier": "^2.3.2",
		"rollup": "^2.56.0",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-terser": "^7.0.2",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"fastify": "^3.20.1"
	},
	"jest": {
		"bail": 5,
		"verbose": false
	},
	"pre-commit": [
		"beautifier",
		"test"
	]
}
